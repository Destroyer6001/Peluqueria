@model TipoDeServicioViewModel
@{
    ViewData["Title"] = "Borrar";
}
<div class="row">
    <div class="col-md-8  offset-md-2 by-10" id="BorrarTipoDeServicio">
        <div class="row" id="TituloBorrarTipoDeServicio">
            <h1>Borrar </h1>
        </div>
        <div class="row" id="SubtituloBorrarTipoDeServicio">
            <h3 class="mb-3">¡Esta seguro que desea borrar el tipo de servicio @Model.Nombre</h3>
        </div>
        <div class="row" id="FormularioBorrarTipoDeServicio">
            <form asp-action="BorrarTipoDeServicio">
                <input type="hidden" asp-for="Id" />
                <button type="submit" class="btn" id="BotonBorrarTipoDeServicio">Borrar</button>
                <a class="btn" asp-action="Index" id="botonCancelarBorrarTipoDeServicio">Cancelar</a>
            </form>
        </div>
    </div>
</div>